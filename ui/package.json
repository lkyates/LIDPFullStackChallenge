{
  "name": "lidp-challenge",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod",
    "build-prod": "ng build --prod --aot=true --build-optimizer=true",
    "dev": "node --max-old-space-size=4096 ./node_modules/@angular/cli/bin/ng serve --host=0.0.0.0 --port=9090",
    "test": "ng test",
    "e2e": "ng e2e --protractorConfig=protractor.conf.js",
    "e2e-dev": "ng e2e --protractorConfig=protractor-dev.conf.js",
    "lint": "./node_modules/.bin/eslint . --ext js,ts",
    "lint-fix": "./node_modules/.bin/eslint . --ext js,ts --fix",
    "lint-staged-local": "lint-staged"
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint-fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run lint"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^11.2.13",
    "@angular/cdk": "^11.2.12",
    "@angular/common": "^11.2.13",
    "@angular/compiler": "^11.2.13",
    "@angular/core": "^11.2.13",
    "@angular/flex-layout": "^11.0.0-beta.33",
    "@angular/forms": "^11.2.13",
    "@angular/material": "^11.2.12",
    "@angular/material-moment-adapter": "^11.2.12",
    "@angular/platform-browser": "^11.2.13",
    "@angular/platform-browser-dynamic": "^11.2.13",
    "@angular/router": "^11.2.13",
    "@circlon/angular-tree-component": "^11.0.4",
    "@swimlane/ngx-charts": "^17.0.1",
    "core-js": "^3.12.0",
    "lodash.clonedeep": "^4.5.0",
    "mat-currency-format": "0.0.7",
    "moment": "^2.29.1",
    "ng2-currency-mask": "^11.0.0",
    "ng2-pdf-viewer": "^6.4.1",
    "ngx-markdown": "^11.1.3",
    "npm": "^7.12.0",
    "rxjs": "^7.0.0",
    "tslib": "^2.2.0",
    "uuid": "^8.3.2",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.12",
    "@angular/cli": "^11.2.12",
    "@angular/compiler-cli": "^11.2.13",
    "@angular/language-service": "^11.2.13",
    "@types/jasmine": "~3.7.0",
    "@types/jasminewd2": "^2.0.9",
    "@types/node": "^15.0.2",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "codelyzer": "^6.0.2",
    "eslint": "^7.25.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jasmine": "^4.1.2",
    "husky": "^6.0.0",
    "jasmine-core": "~3.7.1",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.3",
    "karma-jasmine": "~4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "lint-staged": "^10.5.4",
    "protractor": "~7.0.0",
    "ts-node": "^9.1.1",
    "tslint": "~6.1.3",
    "typescript": "~4.1.5"
  }
